    TARGET_OBJ_VER NUMERIC(10,0) DEFAULT 0 NOT NULL
)
<%if (partition) {
%>ON PS_MTDB (TENANT_ID)
<%}%>GO

CREATE INDEX OBJ_REF_RB${tableNamePostfix}_INDEX1 ON OBJ_REF_RB${tableNamePostfix} (TENANT_ID, OBJ_DEF_ID, RB_ID)
